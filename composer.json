{
  "name": "william-suppo/forge-backup",
  "type": "project",
  "description": "Backup tool for Laravel Forge",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "devmakerlab/entities": "^3.0",
    "guzzlehttp/guzzle": "^7.3",
    "symfony/dependency-injection": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "symfony/var-dumper": "^5.3",
    "vlucas/phpdotenv": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
